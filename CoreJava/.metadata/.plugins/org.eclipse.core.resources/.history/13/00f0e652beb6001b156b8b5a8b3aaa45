package com.slokam.core.innerclasses;

public class AnonymousTest2 {

	public static void main(String[] args) {
		Runnable r = new Runnable() {
			
			@Override
			public void run() {
				for(int i=0;i<10;i++) {
					Thread.currentThread().sleep(1000);
					System.out.println(i);
				}
			}
		};
	}
}
